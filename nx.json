{
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": { "defaultBase": "master" },
  "npmScope": "nps",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "nps-web": { "tags": [] },
    "nps-web-e2e": { "tags": [], "implicitDependencies": ["nps-web"] },
    "nps-api": { "tags": [] },
    "ui": { "tags": ["shared"] },
    "utils": { "tags": ["shared"] }
  }
}
